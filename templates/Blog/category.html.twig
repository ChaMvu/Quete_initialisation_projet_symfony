{% extends 'base.html.twig' %}

{% block body %}

    <h1 class = "text-center" > Category : {{ category.name }}</h1>


    {% for article in articles %}
        <div class="container">
            <h3>{{ article.id }} -  {{ article.title }}</h3>
            <p> {{ article.content }}</p>
        </div>
    {% endfor %}

{% endblock %}